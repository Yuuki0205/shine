<div class='all'>

<h2 class='rog'>ログイン</h2>
 <div class='appleone'> 
<%= image_tag "apple.png", class:"apple-one" %>
 </div> 

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="field">
    <%= f.label "メールアドレス" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label "パスワード" %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  

  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
   <%# <%= link_to image_tag("apple.png", class:"apple-icon"), "/" %> 
   <%= image_tag "apple.png", class:"apple" %>
<% end %>

<%#= render "devise/shared/links" %>
</div>